<beans>
  <bean id="eu.xenit.alfresco.logging.NullBeanDefinitionRegistryPostProcessor" class="eu.xenit.alfresco.logging.NullBeanDefinitionRegistryPostProcessor" depends-on="log4JHierarchyInit"/>
</beans>
